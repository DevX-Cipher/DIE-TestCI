// Detect It Easy: detection rule file

init("compiler", "FASM");

includeScript("FASM");

function detect() {
    if (bFASM) {
        sVersion = PE.getCompilerVersion();
        sOptions = PE.getGeneralOptionsEx(); // FASM doesn't have a linker
        bDetected = true;

        // Correct version
        if (PE.getMajorLinkerVersion() > 2) {
            sVersion += "*";
        }
    }

    sLang = "ASMx" + (PE.is64() ? "64" : "86");

    return result();
}