// http://www.vgrsoft.net/Products/ILProtector
init("protector", "ILProtector");

function detect() {
    bDetected = PE.isNet() && PE.isNetGlobalCctorPresent() && PE.isNetObjectPresent("Protect32.dll") && PE.isNetObjectPresent("Protect64.dll")

    return result();
}