init("library", "SQLite");

function detect() {
    if (PE.isLibraryPresentExp(/sqlite/i)) {
        bDetected = true;
    } else if (PE.isExportFunctionPresentExp(/sqlite3/i)) {
        sVersion = "3";
        bDetected = true;
    }

    return result();
}